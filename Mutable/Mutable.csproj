<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Title>Mutable Library</Title>
        <Authors>Stephen Booth</Authors>
        <PackageProjectUrl>https://github.com/sboothza/mutable</PackageProjectUrl>
        <RepositoryUrl>https://github.com/sboothza/mutable</RepositoryUrl>
        <Company>Stephen Booth</Company>
        <Product>MutableLibrary</Product>
        <PackageVersion>1.0.3</PackageVersion>
        <RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <Description>Mutable string implementation</Description>
        <Copyright>(c) Stephen Booth</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <Optimize>false</Optimize>
    </PropertyGroup>
    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="\" />
    </ItemGroup>
</Project>
